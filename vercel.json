{
  "version": 2,
  "name": "dashcam",
  "builds": [
    {
      "src": "website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/app-ads.txt", "dest": "app-ads.txt" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "index.html" }
  ]
} 